<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Admin.UpdateFoodController">
   <children>
      <VBox layoutX="214.0" layoutY="66.0" prefHeight="368.0" prefWidth="272.0" spacing="15.0">
         <children>
            <Label prefHeight="25.0" prefWidth="272.0" text="Enter new Name:" />
            <TextField fx:id="nameText" />
            <Label layoutX="10.0" layoutY="10.0" prefHeight="25.0" prefWidth="272.0" text="Enter new Type:" />
            <TextField fx:id="typeText" layoutX="10.0" layoutY="35.0" />
            <Label layoutX="10.0" layoutY="60.0" prefHeight="25.0" prefWidth="272.0" text="Enter new Price:" />
            <TextField fx:id="priceText" layoutX="10.0" layoutY="85.0" />
            <Label layoutX="10.0" layoutY="110.0" prefHeight="25.0" prefWidth="272.0" text="Enter new Availability:" />
            <TextField fx:id="availableText" layoutX="10.0" layoutY="135.0" />
            <Label layoutX="10.0" layoutY="160.0" prefHeight="25.0" prefWidth="272.0" text="Enter new imagePath:" />
            <TextField fx:id="imgpathText" layoutX="10.0" layoutY="185.0" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
      <Button layoutX="554.0" layoutY="402.0" mnemonicParsing="false" onAction="#updateFood" prefHeight="32.0" prefWidth="98.0" text="Update" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchScene" prefHeight="25.0" prefWidth="28.0" text="&lt;" />
   </children>
</AnchorPane>
